<div class="container">
 <table class="table table-striped table-dark table-hover text-center">
    <thead>
      <tr>
        <th>FIRSTNAME</th>
        <th>LASTNAME</th>
        <th>EMAIL</th>
        <th>PRODUCT</th>
        <th>QUANTITY</th>
        <th>ORDER DATE</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        <td>{{this.fname}}</td>
        <td>{{this.lname}}</td>
        <td>{{this.email}}</td>
        <td>{{this.product}}</td>
        <td id = "date">{{this.qty}}</td>
        <td>{{this.orderdate}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

<script>
  var today = document.getElementById("date").innerText;
today.setHours(today.getHours() + 8);
document.getElementById("date").innerText = today;
</script>