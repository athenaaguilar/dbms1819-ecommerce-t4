<div class="container">
 <table class="table table-striped table-dark table-hover text-center">
    <thead>
      <tr>
        <th>BRANDS</th>
        <th>DESCRIPTION</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.description}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>